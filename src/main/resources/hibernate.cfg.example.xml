<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQL95Dialect</property>
        <property name="hibernate.format_sql">true</property>
        <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
        <property name="hibernate.connection.url">jdbc:postgresql://nas.loc:5432/iotcore</property>
        <property name="hibernate.jdbc.time_zone">UTC</property>
        <property name="hibernate.connection.requireSSL">false</property>
        <property name="hibernate.connection.ssl">false</property>
        <property name="hibernate.connection.verifyServerCertificate">false</property>
        <property name="hibernate.connection.username">iotcoreuser</property>
        <property name="hibernate.connection.password">password</property>
        <mapping class="com.denysobukhov.mqtt2dbconnector.dao.EnvironmentMessage"/>
        <mapping class="com.denysobukhov.mqtt2dbconnector.dao.SensorMessage"/>
        <mapping class="com.denysobukhov.mqtt2dbconnector.dao.ParameterValue"/>
        <mapping class="com.denysobukhov.mqtt2dbconnector.dao.ParameterName"/>
    </session-factory>
</hibernate-configuration>